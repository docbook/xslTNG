<?xml version="1.0" encoding="utf-8"?>
<article xmlns="http://docbook.org/ns/docbook"  version="5.0" >
  <title>CALS Table with caption</title>
  <table frame="all">
    <info>
      <title>Table title</title>
    </info>
    <tgroup cols="2">
      <tbody>
        <row>
          <entry>A</entry>
          <entry>B</entry>
        </row>
      </tbody>
    </tgroup>
    <caption>
      <para>A <tag>caption</tag> below the table</para>
    </caption>
  </table>
</article>
