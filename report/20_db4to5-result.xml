<?xml version="1.0" encoding="UTF-8"?>
<report xmlns="http://www.jenitennison.com/xslt/xspec"
         xspec="file:/home/runner/work/xslTNG/xslTNG/build/xspec/20_db4to5.xspec"
         stylesheet="file:/home/runner/work/xslTNG/xslTNG/build/xslt/transforms/20-db4to5.xsl"
         date="2023-03-02T09:12:46.153211Z">
   <scenario id="scenario1"
              xspec="file:/home/runner/work/xslTNG/xslTNG/build/xspec/20_db4to5.xspec">
      <label>When transorming a db4 document</label>
      <input-wrap xmlns="">
         <x:call xmlns:db="http://docbook.org/ns/docbook"
                  xmlns:t="http://docbook.org/ns/docbook/templates"
                  xmlns:x="http://www.jenitennison.com/xslt/xspec"
                  template="t:main">
            <x:param name="context">
               <book>
                  <bookinfo>
                     <title>Some Book</title>
                  </bookinfo>
               </book>
               <chapter>
                  <title>A Chapter</title>
                  <para>Content</para>
               </chapter>
            </x:param>
         </x:call>
      </input-wrap>
      <result select="/self::document-node()">
         <content-wrap xmlns=""><!-- DocBook V4.x converted to DocBook V5.x by 10-db4to5.xsl version 1.0 -->
            <ws xmlns="http://www.jenitennison.com/xslt/xspec">
</ws>
            <book xmlns="http://docbook.org/ns/docbook"
                   xmlns:xlink="http://www.w3.org/1999/xlink"
                   version="5.0">
               <info>
                  <title>Some Book</title>
               </info>
            </book>
            <chapter xmlns="http://docbook.org/ns/docbook"
                      xmlns:xlink="http://www.w3.org/1999/xlink"
                      version="5.0">
               <info>
                  <title>A Chapter</title>
               </info>
               <para>Content</para>
            </chapter>
         </content-wrap>
      </result>
      <test id="scenario1-expect1" successful="true">
         <label>the root element to be in the DocBook namespace</label>
         <expect-test-wrap xmlns="">
            <x:expect xmlns:db="http://docbook.org/ns/docbook"
                       xmlns:t="http://docbook.org/ns/docbook/templates"
                       xmlns:x="http://www.jenitennison.com/xslt/xspec"
                       test="exists(/db:*)"/>
         </expect-test-wrap>
         <expect select="()"/>
      </test>
      <test id="scenario1-expect2" successful="true">
         <label>the root element to have a version attribute</label>
         <expect-test-wrap xmlns="">
            <x:expect xmlns:db="http://docbook.org/ns/docbook"
                       xmlns:t="http://docbook.org/ns/docbook/templates"
                       xmlns:x="http://www.jenitennison.com/xslt/xspec"
                       test="/*/@version = '5.0'"/>
         </expect-test-wrap>
         <expect select="()"/>
      </test>
      <test id="scenario1-expect3" successful="true">
         <label>the bookinfo element to be renamed</label>
         <expect-test-wrap xmlns="">
            <x:expect xmlns:db="http://docbook.org/ns/docbook"
                       xmlns:t="http://docbook.org/ns/docbook/templates"
                       xmlns:x="http://www.jenitennison.com/xslt/xspec"
                       test="exists(/*/db:info)"/>
         </expect-test-wrap>
         <expect select="()"/>
      </test>
   </scenario>
</report>
